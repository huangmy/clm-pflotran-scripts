!Description: 3D variably saturated flow with infiltration at top

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE RICHARDS
    /
    SUBSURFACE_TRANSPORT transport
      GLOBAL_IMPLICIT
    /
  /
  CHECKPOINT
      PERIODIC TIMESTEP 100
  /
END

SUBSURFACE

PROC 20 2 1    ! num processes in x, y, z
!ONLY_VERTICAL_FLOW
!DEBUG
!  PRINT_COUPLERS River
!END

!=========================== chemistry ========================================
CHEMISTRY
  PRIMARY_SPECIES
    Tracer
  /
  OUTPUT
    ALL
    TOTAL 
  /
END

!=========================== runtime ==========================================
!WALLCLOCK_STOP 1.e20

!=========================== solver options ===================================
TIMESTEPPER FLOW
  TS_ACCELERATION 8
  MAX_TS_CUTS 5
END

NEWTON_SOLVER FLOW
  MAXIT 20
  RTOL 1.d-50
  ATOL 1.d-50
  STOL 1.e-60
  ITOL_UPDATE 1.d0
!  NO_INFINITY_NORM
!  NO_PRINT_CONVERGENCE
!  PRINT_DETAILED_CONVERGENCE
END

LINEAR_SOLVER FLOW
END

NEWTON_SOLVER TRANSPORT
!  RTOL 1.d-4
!  ATOL 1.d-4
!  STOL 1.e-60
!  DTOL 1.e4
!  ITOL_UPDATE 1.d-4
  NO_INFINITY_NORM
!  NO_PRINT_CONVERGENCE
!  PRINT_DETAILED_CONVERGENCE
END

LINEAR_SOLVER TRANSPORT
END

!=========================== discretization ===================================
GRID
  TYPE structured
  ORIGIN 0.d0 0.d0 90.d0
!changeme - change number of grid cells in each direction
  NXYZ 20 20 62
  BOUNDS
    0.d0 0.0 90.0 
    400.d0 400.0 121.d0
  /
END

!=========================== fluid properties =================================
FLUID_PROPERTY 
  DIFFUSION_COEFFICIENT 1.d-9
END

!=========================== datasets =========================================
DATASET Permeability
  FILENAME ./PriorFields_Plume_CLMPF_20mx20mxhalfm_par1to2.h5
  REALIZATION_DEPENDENT
/

DATASET BC_East
  FILENAME ./BC_UK1_Plume2009-2015.h5
  HDF5_DATASET_NAME BC_East
  MAX_BUFFER_SIZE 200
END

DATASET BC_West
  FILENAME ./BC_UK1_Plume2009-2015.h5
  HDF5_DATASET_NAME BC_West
  MAX_BUFFER_SIZE 200
END

DATASET BC_South
  FILENAME ./BC_UK1_Plume2009-2015.h5
  HDF5_DATASET_NAME BC_South
  MAX_BUFFER_SIZE 200
END

DATASET BC_North
  FILENAME ./BC_UK1_Plume2009-2015.h5
  HDF5_DATASET_NAME BC_North
  MAX_BUFFER_SIZE 200
END

DATASET Initial_Head
  FILENAME ./BC_UK1_Plume2009-2015_Initial_Head.h5
  HDF5_DATASET_NAME Initial_Head
END

!=========================== material properties ==============================
MATERIAL_PROPERTY Hanford
  ID 1
  POROSITY 0.2d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    DATASET Permeability
    !PERM_X 7.387d-9
    !PERM_Y 7.387d-9
    !PERM_Z 7.387d-10
  /
END

MATERIAL_PROPERTY Ringold
  ID 4
  POROSITY 0.43d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf2
  PERMEABILITY
    PERM_X 1.055d-12
    PERM_Y 1.055d-12
    PERM_Z 1.055d-13
  /
END

!=========================== saturation functions =============================
CHARACTERISTIC_CURVES sf1
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.3391d0
    ALPHA  7.2727d-04
    LIQUID_RESIDUAL_SATURATION 0.16d0
    MAX_CAPILLARY_PRESSURE 1.d8
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.3391d0
    LIQUID_RESIDUAL_SATURATION 0.16d0
  /
END

CHARACTERISTIC_CURVES sf2
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.7479d0
    ALPHA  1.4319d-04
    LIQUID_RESIDUAL_SATURATION 0.1299d0
    MAX_CAPILLARY_PRESSURE 1.d8
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.7479d0
    LIQUID_RESIDUAL_SATURATION 0.1299d0
  /
END

!=========================== output options ===================================
OUTPUT
  MASS_BALANCE
!  PERMEABILITY
!  POROSITY
!  TIMES y 0.25d0 0.5d0 0.75d0
!  PERIODIC TIME 1. d
!  FORMAT TECPLOT BLOCK
  FORMAT HDF5 !MULTIPLE_FILES
!  VELOCITIES
   VELOCITY_AT_FACE
! FORMAT TECPLOT BLOCK
  PERIODIC TIME 12 h
  FORMAT HDF5 !MULTIPLE_FILES
  PERIODIC_OBSERVATION TIMESTEP 1
END

!=========================== times ============================================
TIME
  !FINAL_TIME 730.d0 h
  FINAL_TIME 2928.d0 h
  INITIAL_TIMESTEP_SIZE 0.5d0 h
  MAXIMUM_TIMESTEP_SIZE 0.5d0 h
END

!=========================== regions ==========================================
REGION all
  COORDINATES
    0.d0 0.d0 90.d0
    400.d0 400.d0 121.d0
  /
END

REGION bottom
  FACE BOTTOM
  COORDINATES
    0.d0 0.d0 90.d0
    400.d0 400.d0 90.d0
  /
END

REGION Top 
  FACE TOP
  COORDINATES
    0.d0 0.d0 121.d0
    400.d0 400.d0 121.d0
  /
END

REGION South 
  FACE SOUTH
  COORDINATES
    0.d0 0.d0 90.d0
    300.d0 0.d0 121.d0
  /
END

REGION North 
  FACE NORTH
  COORDINATES
    0.d0 400.d0 90.d0
    300.d0 400.d0 121.d0
  /
END

REGION West 
  FACE WEST
  COORDINATES
    0.d0 0.d0 90.d0
    0.d0 400.d0 121.d0
  /
END

REGION River 
  FILE ./clmpf_400x400x31_20mx20mxhalfRes_material_mapped2.h5
END

