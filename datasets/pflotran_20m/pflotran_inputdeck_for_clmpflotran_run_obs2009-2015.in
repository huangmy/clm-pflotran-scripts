!=========================== flow conditions ==================================

MINIMUM_HYDROSTATIC_PRESSURE -1.d0

FLOW_CONDITION initial
  TYPE
    PRESSURE hydrostatic
  /
  CYCLIC
  DATUM DATASET Initial_Head
  !DATUM 0.d0 0.d0 105.d0
  PRESSURE 101325.d0
END

FLOW_CONDITION Recharge
  TYPE
    FLUX neumann
  /
  FLUX 1.757d-9 ! 5.54 cm/yr from Fayer and Walters (1995)
/


FLOW_CONDITION Flow_East
  TYPE
    PRESSURE conductance
  /
  CONDUCTANCE 1.d-12
  CYCLIC
  DATUM file ./DatumH_River_2009-2015.txt
  PRESSURE 101325.d0
  GRADIENT
    PRESSURE file ./Gradients_River_2009-2015.txt
  /
/

FLOW_CONDITION Flow_West
  TYPE
    PRESSURE hydrostatic
  /
  CYCLIC
  DATUM DATASET BC_West
  PRESSURE 101325.d0
/

FLOW_CONDITION Flow_South
  TYPE
    PRESSURE hydrostatic
  /
  CYCLIC
  DATUM DATASET BC_South
  PRESSURE 101325.d0
/

FLOW_CONDITION Flow_North
  TYPE
    PRESSURE hydrostatic
  /
  CYCLIC
  DATUM DATASET BC_North
  PRESSURE 101325.d0
/

FLOW_CONDITION et
  TYPE
    RATE heterogeneous_mass_rate
  /
  RATE 0.0d0
END

!=========================== transport conditions =============================

TRANSPORT_CONDITION Initial
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    0.d0 ifrc_initial
  /
END

TRANSPORT_CONDITION solute
  TYPE dirichlet
  CONSTRAINT_LIST
    0.d0 solute_concentration
  /
END


TRANSPORT_CONDITION Recharge
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    0.d0 ifrc_initial
  /
/


!=========================== constraints ======================================

CONSTRAINT solute_concentration
  CONCENTRATIONS
    Tracer 1.d0 T
  /
END

CONSTRAINT ifrc_initial
  CONCENTRATIONS
    Tracer 1.d-40 T
  /
END



!=========================== condition couplers ===============================
! initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION Initial
  REGION all
END

! east boundary condition
BOUNDARY_CONDITION East
  FLOW_CONDITION Flow_East
  TRANSPORT_CONDITION solute
  REGION River
END

BOUNDARY_CONDITION West
  FLOW_CONDITION Flow_West
  TRANSPORT_CONDITION Initial
  REGION West
/
skip
BOUNDARY_CONDITION East
  FLOW_CONDITION Flow_East
  TRANSPORT_CONDITION Initial
  REGION East
/
noskip
BOUNDARY_CONDITION South
  FLOW_CONDITION Flow_South
  TRANSPORT_CONDITION Initial
  REGION South
/

BOUNDARY_CONDITION North
  FLOW_CONDITION Flow_North
  TRANSPORT_CONDITION Initial
  REGION North
/

!BOUNDARY_CONDITION Recharge
!  FLOW_CONDITION Recharge
!  TRANSPORT_CONDITION Recharge
!  REGION Top
!/

SOURCE_SINK clm_et_ss
  FLOW_CONDITION et
  TRANSPORT_CONDITION Recharge
  REGION all
END


!=========================== stratigraphy couplers ============================
STRATA
  MATERIAL ./clmpf_400x400x31_20mx20mxhalfRes_material_mapped2.h5
END

END_SUBSURFACE

